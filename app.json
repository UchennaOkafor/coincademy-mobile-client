{
  "expo": {
    "name": "Learn",
    "slug": "eko-mobile-client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/app/icon.png",
    "splash": {
      "image": "./app/assets/images/app/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/images/app/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./app/assets/images/app/favicon.png"
    },
    "jsEngine": "hermes"
  }
}
