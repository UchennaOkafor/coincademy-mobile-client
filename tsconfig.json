{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "*": ["app/*"],
      "@app/*": ["app/*"],
      "@assets/*": ["app/assets/*"],
      "@components/*": ["app/components/*"],
      "@screens/*": ["app/screens/*"],
      "@styles/*": ["app/styles/*"],
      "@locales/*": ["app/locales/*"],
      "@models/*": ["app/models/*"],
      "@services/*": ["app/services/*"],
      "@codegen": ["./app/codegen"],
    }
  }
}
